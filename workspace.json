{
  "version": 2,
  "projects": {
    "my-homepage-backend": {
      "root": "apps/my-homepage-backend",
      "sourceRoot": "apps/my-homepage-backend/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/my-homepage-backend",
            "main": "apps/my-homepage-backend/src/main.ts",
            "tsConfig": "apps/my-homepage-backend/tsconfig.app.json",
            "assets": [
              "apps/my-homepage-backend/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/my-homepage-backend/src/environments/environment.ts",
                  "with": "apps/my-homepage-backend/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "my-homepage-backend:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/my-homepage-backend/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/my-homepage-backend"
          ],
          "options": {
            "jestConfig": "apps/my-homepage-backend/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "my-homepage-nuxt-frontend": {
      "projectType": "application",
      "root": "apps/my-homepage-nuxt-frontend",
      "targets": {
        "build": {
          "executor": "@nx-plus/nuxt:browser",
          "options": {
            "buildDir": "dist/apps/my-homepage-nuxt-frontend"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nx-plus/nuxt:server",
          "options": {
            "browserTarget": "my-homepage-nuxt-frontend:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "browserTarget": "my-homepage-nuxt-frontend:build:production",
              "dev": false
            }
          }
        },
        "static": {
          "executor": "@nx-plus/nuxt:static",
          "options": {
            "browserTarget": "my-homepage-nuxt-frontend:build:production"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/my-homepage-nuxt-frontend/**/*.{ts,js,vue}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/my-homepage-nuxt-frontend"
          ],
          "options": {
            "jestConfig": "apps/my-homepage-nuxt-frontend/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "my-homepage-nuxt-frontend-e2e": {
      "root": "apps/my-homepage-nuxt-frontend-e2e",
      "sourceRoot": "apps/my-homepage-nuxt-frontend-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/my-homepage-nuxt-frontend-e2e/cypress.json",
            "devServerTarget": "my-homepage-nuxt-frontend:serve",
            "tsConfig": "apps/my-homepage-nuxt-frontend-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "my-homepage-nuxt-frontend:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/my-homepage-nuxt-frontend-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "my-homepage-vue2-frontend": {
      "projectType": "application",
      "root": "apps/my-homepage-vue2-frontend",
      "sourceRoot": "apps/my-homepage-vue2-frontend/src",
      "targets": {
        "build": {
          "executor": "@nx-plus/vue:browser",
          "options": {
            "dest": "dist/apps/my-homepage-vue2-frontend",
            "index": "apps/my-homepage-vue2-frontend/public/index.html",
            "main": "apps/my-homepage-vue2-frontend/src/main.ts",
            "tsConfig": "apps/my-homepage-vue2-frontend/tsconfig.app.json"
          },
          "configurations": {
            "production": {
              "mode": "production",
              "filenameHashing": true,
              "productionSourceMap": true,
              "css": {
                "extract": true,
                "sourceMap": false
              }
            }
          }
        },
        "serve": {
          "executor": "@nx-plus/vue:dev-server",
          "options": {
            "browserTarget": "my-homepage-vue2-frontend:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "my-homepage-vue2-frontend:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/my-homepage-vue2-frontend/**/*.{ts,tsx,vue}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/my-homepage-vue2-frontend"
          ],
          "options": {
            "jestConfig": "apps/my-homepage-vue2-frontend/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "my-homepage-vue2-frontend-e2e": {
      "root": "apps/my-homepage-vue2-frontend-e2e",
      "sourceRoot": "apps/my-homepage-vue2-frontend-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/my-homepage-vue2-frontend-e2e/cypress.json",
            "devServerTarget": "my-homepage-vue2-frontend:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "my-homepage-vue2-frontend:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/my-homepage-vue2-frontend-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/nest"
  },
  "defaultProject": "my-homepage-backend"
}
